{
  "name": "Rate my Professor Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "rmp.png"
  },
    "web_accessible_resources": [
    {
      "resources": ["data.json"],
      "matches": ["<all_urls>"]
    }
  ]
}